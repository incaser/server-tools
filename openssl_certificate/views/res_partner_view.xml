<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>

    <record id="openssl_create_certificates" model="ir.actions.server">
        <field name="name">Create x509 Certificate</field>
        <field name="model_id" ref="base.model_res_partner"/>
        <field name="code">action=self.pool['openssl.certificate'].mass_create_certificate(cr, uid, context=context)</field>
    </record>

    <record id="action_partner_create_openssl_certificate" model="ir.values">
      <field name="name">Create x509 Certificate</field>
      <field name="key2">client_action_multi</field>
      <field name="value" eval="'ir.actions.server,' +str(ref('openssl_create_certificates'))" />
      <field name="model">res.partner</field>
    </record>

</data>
</openerp>